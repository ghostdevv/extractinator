{
    "htmlWhitespaceSensitivity": "strict",
    "quoteProps": "as-needed",
    "arrowParens": "always",
    "trailingComma": "all",
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 100,
    "useTabs": true,
    "tabWidth": 4,
    "semi": false,
    "svelteSortOrder": "scripts-options-markup-styles",
    "svelteAllowShorthand": true,
    "svelteBracketNewLine": false,
    "svelteIndentScriptAndStyle": true
}